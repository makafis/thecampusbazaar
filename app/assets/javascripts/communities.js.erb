$(document).ready(function(){
  // var items = $(".itembox")

    $('#filters :checkbox').click(function () {
        if ($('input:checkbox:checked').length) {
            $('.itembox').hide();
            $('input:checkbox:checked').each(function () {
                $('.itembox[data-price' + '*="' + $(this).val() + '"]').show();
            });
        } else {
            $(".itembox").show();
        }
    });
});
